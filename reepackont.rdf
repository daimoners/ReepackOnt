    <!-- http://www.w3.org/ns/sosa/detects -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/detects">
        <owl:inverseOf rdf:resource="http://www.w3.org/ns/sosa/isDetectedBy"/>
        <schema:domainIncludes rdf:resource="http://www.w3.org/ns/sosa/Sensor"/>
        <schema:rangeIncludes rdf:resource="http://www.w3.org/ns/sosa/Stimulus"/>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/obs/"/>
        <rdfs:label xml:lang="en">detects</rdfs:label>
        <skos:definition xml:lang="en">
  A relation from a Sensor to the Stimulus that the Sensor detects.
  The Stimulus itself will be serving as a proxy for some Property.
  </skos:definition>
    </owl:ObjectProperty>

    <!-- http://www.w3.org/ns/sosa/isDetectedBy -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/isDetectedBy">
        <schema:domainIncludes rdf:resource="http://www.w3.org/ns/sosa/Stimulus"/>
        <schema:rangeIncludes rdf:resource="http://www.w3.org/ns/sosa/Sensor"/>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/obs/"/>
        <rdfs:label xml:lang="en">is detected by</rdfs:label>
        <skos:definition xml:lang="en">
  A relation from a Stimulus to the Sensor that may detect it.
  The Stimulus itself will be serving as a proxy for some Property.
  </skos:definition>
    </owl:ObjectProperty>


